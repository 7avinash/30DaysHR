#include <sstream>
#include <iostream>
#include <algorithm>
using namespace std;


int main(){
    int A[6][6],sum=0,i,j,k=0,ans=INT_MIN;
    for(i=0;i<6;i++)
    {
        for(j=0;j<6;j++)
        {
            cin>>A[i][j];
        }
    }




    for(i=0;i<4;i++)
    {
        for(j=0;j<4;j++)
        {
            sum=A[i][j]+A[i][j+1]+A[i][j+2]+A[i+1][j+1]+A[i+2][j]+A[i+2][j+1]+A[i+2][j+2];
            if(sum>ans)
                ans=sum;
        }
    }
    
    cout<<ans;
    return 0;
}
